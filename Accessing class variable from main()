public class Main
{
    public static int  x=10;
    public static  void repeat()
    {
        for(int i=0;i<2;i++)
        {
            x++;
        }
    }
	public static void main(String[] args) {
	    repeat();//Accessing class variable from main();
		System.out.print(x);//Accessing class variable from main();
	}
}
